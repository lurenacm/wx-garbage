.container{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  width: 300rpx;
  height: 300rpx;
  border-radius: 20rpx;
  box-shadow: 1px 1px 1px 1px  #fcfcfc;
  border: 1px solid #fcfcfc;
  margin:20rpx 35rpx;
  background-color: #fff;
}

.img {
  padding: 5rpx;
  width: 250rpx;
  height: 250rpx;
}

.text-font{
  font-size: 14px;
  font-weight: 500;
}

.text-nums {
  color: red;
  font-weight: 600;
}